.Foot
  background-color footerColor
  .Mobile
    wrap()
    wrap 'center'
    size 100% 475px
    .col
      col()
      col(1,1)
    .brandLogo
      wrap()
      wrap('center')
      margin 35px auto
      img
        size auto 75px
    .socialMedia
      wrap()
      wrap('center')
      i.fa
        font-size 20px
        color darkBlue
        hover-color: lightBlue true
        margin auto 10px
    .footer
      font-size 12px
      color darkBlue
    .bhhs
      wrap()
      wrap('center')
      img
        size auto 60px
    .dev
      color darkBlue
      font-size 12px
  .Tablet
    wrap()
    wrap('center')
    size 1200px auto
    max-width: 100%
    margin auto
    .brandLogo
      col()
      col(3,12)
      a
        wrap()
        wrap('center')
      img
        size auto 75px
    .bhhs
      col()
      col(3,12)
      a
        wrap()
        wrap('center')
      img
        size auto 55px
    .midContent
      col()
      col(6,12)
      .socialMedia
        i.fa
          margin 35px 10px 20px
      .footer
        p
          font-size 10px
          font-family: mainFont
          color darkBlue
          margin 0px auto
      .dev
        font-size 10px
        font-family: mainFont
        p
          margin 0px auto
