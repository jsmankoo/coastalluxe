.Contact
  .Top
    .BGVideo
      overflow hidden
      position relative
      background-color rgba(darkBlue, 0.5)
      +below(767px)
        size 100% 470px
      +above(768px)
        size 100% 525px
      .video-background
        z-index -10000
        align(b)
        min-width: 100%
        min-height: 100%
        size auto auto
    .BGContent
      align(b)
      size 100% auto
    .Mobile
      .BGContent
        .name
          color white
          font-family: otherFont
          font-weight: 700
          font-size: 40px
          text-align: center
          padding 70px 0px
        .hidden
          size 100% 40px
        .info
          center()
          background-color rgba(white, 0.50)
          color darkBlue
          text-align: center
          font-family: otherFont
          font-weight: 500;
          font-size 20px
          padding-top 20px
          padding-bottom 20px
          .wrapper
            column(1)
            .icon
              column(1/5)
              text-align: center
              font-size 22px
              padding 5px 0px
            .content
              column(4/5)
              font-size 14px
              padding 5px 0px

    .Tablet
      .BGContent
        .name
          color white
          font-family: otherFont
          font-weight: 700
          font-size: 60px
          text-align: center
          padding 70px
        .hidden
          size 100% 120px
        .info
          background-color: rgba(white, 0.50)
          center(1280px)
          height 150px
          color darkBlue
          text-align: center
          font-family: otherFont
          font-weight: 500;
          font-size 30px
          .col
            span(1/3)
            height 100%
            position: relative
            .wrapper
              align(v)
              width 100%
              .icon
                font-size: 30px
                padding-bottom 10px
              .content
                text-align: center
                font-size: 18px
